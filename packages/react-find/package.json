{
    "name": "react-find",
    "version": "2.0.4",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "react",
        "react-find",
        "react find",
        "react dev"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./next": {
            "types": "./dist/next/index.d.ts",
            "import": "./dist/next/index.js",
            "require": "./dist/next/index.cjs"
        },
        "./vite": {
            "types": "./dist/vite/index.d.ts",
            "import": "./dist/vite/index.js",
            "require": "./dist/vite/index.cjs"
        },
        "./webpack": {
            "types": "./dist/webpack/index.d.ts",
            "import": "./dist/webpack/index.js",
            "require": "./dist/webpack/index.cjs"
        },
        "./webpack/webpack-react-source-loader": {
            "types": "./dist/webpack/webpack-react-source-loader.d.ts",
            "import": "./dist/webpack/webpack-react-source-loader.js",
            "require": "./dist/webpack/webpack-react-source-loader.cjs"
        },
        "./webpack/webpack-react-source-loader.cjs": "./dist/webpack/webpack-react-source-loader.cjs"
    },
    "typesVersions": {
        "*": {
            "next": [
                "./dist/next/index.d.ts"
            ],
            "vite": [
                "./dist/vite/index.d.ts"
            ],
            "webpack": [
                "./dist/webpack/index.d.ts"
            ]
        }
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mjw-git/react-find"
    },
    "scripts": {
        "dev": "vite",
        "build": "rm -rf dist&&tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview"
    },
    "files": [
        "dist/",
        "LICENSE",
        "README.md",
        "README.zh-CN.md"
    ],
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.26.0",
        "@babel/types": "^7.26.7",
        "@eslint/js": "^9.17.0",
        "@types/babel__core": "^7.20.5",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.17.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.14.0",
        "prettier": "^3.4.2",
        "source-map": "^0.7.4",
        "tapable": "^2.2.1",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.18.2",
        "vite": "^6.0.5",
        "vite-plugin-dts": "^4.4.0",
        "vite-plugin-require": "^1.2.14",
        "webpack": "^5.90.3"
    }
}